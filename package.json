{
  "name": "11ty-fullscreen",
  "version": "1.0.0",
  "description": "fullscreen view with some buttons",
  "main": "index.js",
  "scripts": {
    "eleventy:start": "eleventy --serve --watch",
    "eleventy:build": "eleventy",
    "css": "npx postcss src/css/*.css --dir dist/css",
    "clean": "rm -r -f dist && mkdir dist",
    "start": "npm run clean && cp src/index.html dist/index.html && concurrently 'npx serve dist' 'npm run css -- --watch'"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "clean-css": "^5.3.2",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-js": "^4.0.1",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.3.5"
  },
  "keywords": [],
  "author": "joachim kliemann",
  "license": "ISC",
  "engines": {
    "node": ">=18.x.x"
  }
}
